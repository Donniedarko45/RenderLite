{
  "name": "@renderlite/worker",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "dotenv -e ../../.env -- tsx watch src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "db:generate": "dotenv -e ../../.env -- prisma generate --schema ../api/prisma/schema.prisma",
    "start": "dotenv -e ../../.env -- node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@renderlite/shared": "*",
    "bullmq": "^5.1.1",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.1",
    "ioredis": "^5.3.2",
    "simple-git": "^3.22.0"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.23",
    "@types/node": "^20.11.16",
    "dotenv-cli": "^11.0.0",
    "prisma": "^6.14.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
